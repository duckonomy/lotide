ers\\n\nCreate the mixer and synths\\n\nSet the number of threads\\n\nRun parallel block\n- Calculate an offset into the song for a round effect\n- Play the midi events","declarations":[],"spell":"27:5-27:9|-1|1|2","extent":"27:1-64:2|-1|1|0","bases":[],"derived":[],"vars":[0,1,2,3,4],"uses":[],"callees":["43:3-43:22|1|3|36","47:14-47:32|2|3|36","47:14-47:32|2|3|36"]},{"id":1,"usr":2345827821909463825,"detailed_name":"","short_name_offset":0,"short_name_size":0,"kind":0,"storage":0,"hover":"","comments":"","declarations":[],"bases":[],"derived":[],"vars":[],"uses":["43:3-43:22|0|3|36"],"callees":[]},{"id":2,"usr":17201552877273152228,"detailed_name":"","short_name_offset":0,"short_name_size":0,"kind":0,"storage":0,"hover":"","comments":"","declarations":[],"bases":[],"derived":[],"vars":[],"uses":["47:14-47:32|0|3|36"],"callees":[]}],"vars":[{"id":0,"usr":13549168240179544205,"detailed_name":"int argc","short_name_offset":4,"short_name_size":4,"hover":"","comments":"","declarations":[],"spell":"27:14-27:18|0|3|2","extent":"27:10-27:18|0|3|0","type":0,"uses":["28:7-28:11|0|3|12"],"kind":253,"storage":1},{"id":1,"usr":2024796718510451156,"detailed_name":"char *[] argv","short_name_offset":9,"short_name_size":4,"hover":"","comments":"","declarations":[],"spell":"27:26-27:30|0|3|2","extent":"27:20-27:32|0|3|0","type":1,"uses":[],"kind":253,"storage":1},{"id":2,"usr":16357023044643928838,"detailed_name":"const int numVoices","short_name_offset":10,"short_name_size":9,"hover":"","comments":"","declarations":[],"spell":"37:13-37:22|0|3|2","extent":"37:3-37:22|0|3|0","type":0,"uses":["43:23-43:32|0|3|12"],"kind":13,"storage":1},{"id":3,"usr":8418056907139522148,"detailed_name":"int mixer","short_name_offset":4,"short_name_size":5,"hover":"","comments":"","declarations":[],"spell":"40:15-40:20|0|3|2","extent":"40:3-40:20|0|3|0","type":0,"uses":[],"kind":13,"storage":1},{"id":4,"usr":7983445624572507459,"detailed_name":"int id","short_name_offset":4,"short_name_size":2,"hover